{
  "name": "@codetanzania/emis-questionnaire",
  "version": "1.2.3",
  "description": "A representation of indicators, questions and questionnaires used to assess need and situation of an emergency(or disaster) event.",
  "main": "index.js",
  "scripts": {
    "test": "grunt test",
    "integration": "grunt integration",
    "dev": "BASE_PATH=./examples node ./examples/app.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CodeTanzania/emis-questionnaire.git"
  },
  "keywords": [
    "codetanzania",
    "emis",
    "emis-module",
    "disaster",
    "hazard",
    "risk",
    "emergency",
    "incident",
    "rapid",
    "assessment",
    "questionnaire",
    "question"
  ],
  "contributors": [
    {
      "name": "lykmapipo",
      "email": "lallyelias87@gmail.com",
      "url": "https://github.com/lykmapipo"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CodeTanzania/emis-questionnaire/issues",
    "email": "lallyelias87@gmail.com"
  },
  "homepage": "https://github.com/CodeTanzania/emis-questionnaire",
  "sandbox": {
    "url": "https://emis-questionnaire.herokuapp.com",
    "apiDoc": "https://codetanzania.github.io/emis-questionnaire/",
    "client_id": "",
    "client_secret": "",
    "urls": [
      {
        "resource": "Indicator",
        "schema": "https://emis-questionnaire.herokuapp.com/v1/indicators/schema",
        "link": "https://emis-questionnaire.herokuapp.com/v1/indicators",
        "scopes": [
          "indicator:*"
        ],
        "methods": [
          "get",
          "post",
          "put",
          "patch"
        ]
      },
      {
        "resource": "Question",
        "schema": "https://emis-questionnaire.herokuapp.com/v1/questions/schema",
        "link": "https://emis-questionnaire.herokuapp.com/v1/questions",
        "scopes": [
          "question:*"
        ],
        "methods": [
          "get",
          "post",
          "put",
          "patch"
        ]
      },
      {
        "resource": "Questionnaire",
        "schema": "https://emis-questionnaire.herokuapp.com/v1/questionnaires/schema",
        "link": "https://emis-questionnaire.herokuapp.com/v1/questionnaires",
        "scopes": [
          "questionnaire:*"
        ],
        "methods": [
          "get",
          "post",
          "put",
          "patch"
        ]
      }
    ]
  },
  "devDependencies": {
    "@benmaruchu/faker": ">=4.2.1",
    "@codetanzania/majifix-common": "^0.12.4",
    "@lykmapipo/mongoose-test-helpers": ">=0.7.4",
    "chai": "^4.2.0",
    "grunt": "^1.0.4",
    "grunt-apidoc": "^0.11.0",
    "grunt-contrib-jshint": "^2.1.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-mocha-test": "^0.13.3",
    "jshint-stylish": "^2.2.1",
    "mocha": "^6.1.4",
    "mongoose": ">=5.5.11",
    "supertest": "^4.0.2"
  },
  "peerDependencies": {
    "mongoose": ">=5.5.11"
  },
  "dependencies": {
    "@lykmapipo/common": ">=0.19.0",
    "@lykmapipo/env": ">=0.7.8",
    "@lykmapipo/express-common": ">=0.17.3",
    "@lykmapipo/include": ">=0.2.4",
    "@lykmapipo/mongoose-common": ">=0.23.4",
    "async": ">=3.0.1",
    "lodash": ">=4.17.11",
    "mongoose-locale-schema": ">=0.3.1",
    "mongoose-rest-actions": ">=0.28.1",
    "randomcolor": ">=0.5.4"
  },
  "engines": {
    "node": ">=8.11.1",
    "npm": ">=5.6.0"
  }
